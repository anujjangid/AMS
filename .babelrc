{
  "presets": [
    "module:metro-react-native-babel-preset"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "componets": "./src/components",
          "constants": "./src/constants",
          "dataSet": "./src/data",
